IO_FS_CREATE DIALFS cero.txt
IO_FS_CREATE DIALFS uno.txt
SET ECX 256
IO_FS_TRUNCATE DIALFS uno.txt ECX
IO_FS_CREATE DIALFS cinco.txt
SET EDX 512
IO_FS_TRUNCATE DIALFS cinco.txt EDX
IO_FS_CREATE DIALFS trece.txt
SET ECX 64
IO_FS_TRUNCATE DIALFS uno.txt ECX
IO_FS_CREATE DIALFS dos.txt
IO_FS_DELETE DIALFS dos.txt
IO_FS_CREATE DIALFS dosDOS.txt
EXIT